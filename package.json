{
  "name": "remote-control",
  "version": "1.0.0",
  "description": "用electron开发的远程控制项目。",
  "main": "app/main/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run start:render\" \"wait-on http://localhost:3000 && npm run start:main\"",
