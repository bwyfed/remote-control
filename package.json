  },
  "homepage": "https://github.com/bwyfed/remote-control#readme",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "electron": "^13.1.5",
    "electron-rebuild": "^2.3.5",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "robotjs": "^0.6.0",
    "vkey": "^1.0.1"
